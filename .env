# VITE_FLOWW_EMPLOYEE_TOKEN=removed_use_cookies_instead
# VITE_FLOWW_ADMIN_TOKEN=removed_use_cookies_instead

# Admin Dashboard Configuration
VITE_ADMIN_DASHBOARD_URL=http://localhost:8000/crm/dashboard
VITE_EMPLOYEE_DASHBOARD_URL=http://localhost:8000/employee/dashboard
VITE_ADMIN_PORT=8000

# API Configuration
VITE_API_BASE_URL=https://dev.gofloww.co/api/wall
VITE_API_TIMEOUT=10000

# Default Redirect URLs
VITE_DEFAULT_REDIRECT_URL=https://dev.gofloww.co
VITE_ADMIN_REDIRECT_URL=http://localhost:8000/crm/dashboard

# Environment Configuration
NODE_ENV=development
VITE_APP_TITLE=Atom Buzz
VITE_APP_VERSION=1.0.0

# Performance Settings
VITE_CHUNK_SIZE_WARNING_LIMIT=1000
VITE_ENABLE_SOURCE_MAPS=true

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=false
VITE_ENABLE_PWA=true

# Security
VITE_CSP_ENABLED=true
VITE_HTTPS_ONLY=false
